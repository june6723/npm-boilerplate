{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["DOM", "ESNext"] /* 전역변수 어떤거 필요함? */,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,

		"declaration": true,
		"outDir": "./dist",
		"rootDir": "./src"
	}
}
